  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "typeRoots": ["@types", "node_modules/@types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "**/*.m3u",
    "**/*.m3u8"
  ]
}